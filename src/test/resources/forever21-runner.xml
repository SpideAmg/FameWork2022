<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Forever21 smoke">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="99"></parameter>
    <parameter name="url" value="https://www.forever21.com/"></parameter>

    <test name="Forever21 HomePageTest">
        <classes>
            <class name="Forever21.HomePageTest"></class>
        </classes>
    </test>

    <test name="Forever21.WomenPageTest">
        <classes>
            <class name="Forever21.WomenPageTest"></class>
        </classes>
    </test>

    <test name="LoginPageTesting">
        <classes>
            <class name="Forever21.LoginPageTest"></class>
        </classes>
    </test>

    <test name="GiftCardPageTesting">
        <classes>
            <class name="Forever21.GiftCardPageTest"></class>
        </classes>
    </test>

<!--    <test name="HoverOverPage">-->
<!--        <classes>-->
<!--            <class name="Forever21.HoverOver"></class>-->
<!--        </classes>-->
<!--    </test>-->


</suite>
































